{
    "name": "wonderland-react-ui-screenspace-example",
    "version": "1.0.0",
    "description": "Example to demo Wonderland React UI.",
    "main": "js/index.js",
    "type": "module",
    "module": "js/index.js",
    "scripts": {
        "bootstrap": "npm install --workspaces=false",
        "build:wl": "wonderland-editor --package --windowless --project ReactUi_example_screenspace.wlp",
        "update-reactui": "npm update @wonderlandengine/react-ui --workspaces=false"
    },
    "keywords": [
        "wonderland-engine"
    ],
    "dependencies": {
        "@types/react-reconciler": "0.28.0",
        "@wonderlandengine/api": "^1.1.0",
        "@wonderlandengine/components": "^1.1.5",
        "@wonderlandengine/prettier-config": "^1.0.0",
        "@wonderlandengine/react-ui": "file:../../packages/react-ui",
        "gl-matrix": "^3.4.3"
    },
    "overrides": {
        "@wonderlandengine/api": "$@wonderlandengine/api"
    },
    "devDependencies": {
        "@wonderlandengine/cli": "^0.1.4"
    }
}
